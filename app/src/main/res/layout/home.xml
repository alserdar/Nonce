<android.support.design.internal.ForegroundLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    android:weightSum="3"
    android:background="@color/white"
    tools:context="twitter.nonce_app.Home$OurHome">


    <android.support.design.internal.ForegroundLinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="3"
        android:orientation="horizontal"
        android:weightSum="2">

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical"
            android:weightSum="3">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:text="@string/call_someone"
                android:id="@+id/callSomeoneTextView"
                android:gravity="center"
                android:background="?attr/selectableItemBackground"
                android:clickable="true"
                android:drawableStart="@mipmap/call_someone"
                android:drawableLeft="@mipmap/call_someone"
                android:layout_marginLeft="10dp"
                android:layout_marginStart="10dp"/>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@drawable/line_custome"
                android:layout_marginLeft="10dp"
                android:layout_marginStart="10dp"
                android:visibility="gone" />


            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:text="@string/hangout"
                android:id="@+id/hangTextView"
                android:gravity="center"
                android:background="?attr/selectableItemBackground"
                android:clickable="true"
                android:drawableStart="@mipmap/hangout"
                android:drawableLeft="@mipmap/hangout"
                android:layout_marginLeft="10dp"
                android:layout_marginStart="10dp"
                android:visibility="gone"/>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@drawable/line_custome"
                android:layout_marginLeft="10dp"
                android:layout_marginStart="10dp"/>




            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:text="@string/tweet"
                android:id="@+id/twitterTextView"
                android:gravity="center"
                android:background="?attr/selectableItemBackground"
                android:clickable="true"
                android:drawableStart="@mipmap/twitter"
                android:drawableLeft="@mipmap/twitter"
                android:layout_marginLeft="10dp"
                android:layout_marginStart="10dp"/>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@drawable/line_custome"
                android:layout_marginLeft="10dp"
                android:layout_marginStart="10dp"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:text="@string/free_time"
                android:id="@+id/freeTimeTextView"
                android:drawableLeft="@mipmap/evolve"
                android:drawableStart="@mipmap/evolve"
                android:gravity="center"
                android:clickable="true"
                android:background="?attr/selectableItemBackground"
                android:layout_marginLeft="10dp"
                android:layout_marginStart="10dp"/>
        </LinearLayout>
        <View
            android:layout_width="2dp"
            android:layout_height="match_parent"
            android:background="@drawable/line_custome"
            />
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical"
            android:weightSum="3">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:text="@string/send_sms"
                android:id="@+id/postOfficeTextView"
                android:gravity="center"
                android:background="?attr/selectableItemBackground"
                android:clickable="true"
                android:drawableLeft="@mipmap/post_office"
                android:drawableStart="@mipmap/post_office"
                android:layout_marginLeft="10dp"
                android:layout_marginStart="10dp"/>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@drawable/line_custome"
                android:layout_marginRight="10dp"
                android:layout_marginEnd="10dp"
                android:visibility="gone"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:text="@string/travel"
                android:id="@+id/travelTextView"
                android:gravity="center"
                android:background="?attr/selectableItemBackground"
                android:clickable="true"
                android:drawableLeft="@mipmap/travel"
                android:drawableStart="@mipmap/travel"
                android:layout_marginLeft="10dp"
                android:layout_marginStart="10dp"
                android:visibility="gone"/>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@drawable/line_custome"
                android:layout_marginRight="10dp"
                android:layout_marginEnd="10dp"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:text="@string/updateStatus"
                android:id="@+id/updateStatusTextView"
                android:background="?attr/selectableItemBackground"
                android:clickable="true"
                android:gravity="center_vertical|center_horizontal"
                android:drawableLeft="@mipmap/facebook"
                android:drawableStart="@mipmap/facebook"
                android:layout_marginLeft="10dp"
                android:layout_marginStart="10dp"/>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@drawable/line_custome"
                android:layout_marginRight="10dp"
                android:layout_marginEnd="10dp"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:text="@string/make_event"
                android:id="@+id/otherTextView"
                android:background="?attr/selectableItemBackground"
                android:clickable="true"
                android:gravity="center_vertical|center_horizontal"
                android:drawableLeft="@mipmap/pin"
                android:drawableStart="@mipmap/pin"
                android:layout_marginLeft="10dp"
                android:layout_marginStart="10dp"/>

        </LinearLayout>


    </android.support.design.internal.ForegroundLinearLayout>


</android.support.design.internal.ForegroundLinearLayout>
